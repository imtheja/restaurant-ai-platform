    "react-router-dom": "^6.8.1",
    "@mui/material": "^5.15.2",
    "@mui/icons-material": "^5.15.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.6.2",
    "zustand": "^4.4.7",
    "react-query": "^3.39.3",
    "@types/uuid": "^9.0.7",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "react-hot-toast": "^2.4.1",
    "framer-motion": "^10.16.16"
